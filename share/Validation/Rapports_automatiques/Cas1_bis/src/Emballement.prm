	 casTest . VEF_3D.data    
	}
Chapitre {
	titre "Convergence"
	# definition figure 0
	Figure {
		titre "Evolution of residue"
		labelX "TIME"
		labelY "residu=max|Ri|"
		logY True
		inclureDescCourbes 0
                 Courbe {
			fichier VEF.dt_ev
			colonnes ($1) ($4)
			style linespoints
		}
		 Courbe {
			fichier EF.dt_ev
			colonnes ($1) ($4)
			style linespoints
		}
		 Courbe {
			fichier VEF_3D.dt_ev
			colonnes ($1) ($4)
			style linespoints
		}
			}
}
Chapitre {
	titre "Visualization via VisIt"
