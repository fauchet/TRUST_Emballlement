	Visu {
		titre "MESH"
		mesh VEF.lata dom
	}
	# definition figure 2
	Visu {
		titre "TEMPERATURE SOM"
		pseudocolor VEF.lata dom TEMPERATURE SOM
	}
	# definition figure 3
	Visu {
		titre "CONCENTRATION SOM"
		pseudocolor VEF.lata dom CONCENTRATION SOM
	}
}
